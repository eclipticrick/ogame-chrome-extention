{
  "name": "Zooi alliance united",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Ogame extention for the zooi alliance",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "512": "icons/icon512.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://ogame.gameforge.com/*",
    "https://*.ogame.gameforge.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://ogame.gameforge.com/*",
        "https://*.ogame.gameforge.com/*"
      ],
      "js": [
        "src/inject/functions/taskQueuingElement.js",
        "src/inject/functions/scrapFinderElement.js",
        "src/inject/functions/inactivePlayerFinderElement.js",
        "src/inject/functions/extendedPlayerInformationElement.js",
        "src/inject/functions/element.js",
        "src/inject/inject.js"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}
